{
    "namespace": "pm",
    "metadata": {
        "name": "via-uat-osaka",
        "namespace": null,
        "labels": {"via-uat-version": "1.0"},
        "annotations": {},
        "description": "VIA UAT Testing Demo",
        "disable": null
    },
    "spec": {
        "service": {
            "num_replicas": 1,
            "containers": [
                {
                    "name": "viauatapp",
                    "image": {
                        "name": "jgruberf5/viauatapp:latest",
                        "container_registry": {
                            "tenant": "f5-dataai-ybdklqyy",
                            "namespace": "pm",
                            "name": "dockerhub-jgruberf5"
                        },
                        "pull_policy": "IMAGE_PULL_POLICY_DEFAULT"
                    },
                    "init_container": null,
                    "flavor": "CONTAINER_FLAVOR_TYPE_TINY",
                    "liveness_check": null,
                    "readiness_check": null,
                    "command": null,
                    "args": null
                }
            ],
            "volumes": null,
            "configuration": {
                "parameters": [
                    {
                        "env_var": {
                            "name": "CONFIG_FILE",
                            "value": "https://raw.githubusercontent.com/jgruberf5/ViaUATEndpoint/main/configs/osaka-config.yaml"
                        }
                    }
                ]
            },
            "deploy_options": {
                "deploy_re_sites": {
                    "site": [
                        {
                            "tenant": "ves-io",
                            "namespace": "system",
                            "name": "os1-osa"
                        }
                    ]
                }
            },
            "advertise_options": {
                "advertise_in_cluster": {
                    "port": {
                        "info": {
                            "port": 8000,
                            "protocol": "PROTOCOL_TCP",
                            "same_as_port": {}
                        }
                    }
                }
            }
        }
    }
}
